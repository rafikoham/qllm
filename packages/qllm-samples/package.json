{
    "name": "qllm-samples",
    "version": "1.0.5",
    "description": "QLLM Samples",
    "keywords": [
        "ai",
        "llm",
        "qllm",
        "library",
        "typescript",
        "aws-sdk"
    ],
    "type": "commonjs",
    "main": "dist/index.js",
